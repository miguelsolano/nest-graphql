{
  "name": "@msolano/nest-graphql",
  "version": "1.0.0",
  "description": "A NestJS GraphQL package to facilitate development of resolver and use nestjsx/crud.",
  "author": "Miguel Solano <m.a.solano93@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "main": "dist/test.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miguelsolano/nest-graphql"
  },
  "bugs": "https://github.com/miguelsolano/nest-graphql",
  "peerDependencies": {
    "@nestjs/common": "^6.11.7",
    "@nestjs/core": "^6.11.7",
    "@nestjs/graphql": "^7.1.5",
    "@nestjs/typeorm": "^6.3.1",
    "@nestjsx/crud": "^4.4.2",
    "@nestjsx/crud-typeorm": "^4.4.5"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.7",
    "@nestjs/core": "^6.11.7",
    "@nestjs/graphql": "^7.1.5",
    "@nestjs/typeorm": "^6.3.1",
    "@nestjsx/crud": "^4.4.2",
    "@nestjsx/crud-typeorm": "^4.4.5",
    "install": "^0.13.0",
    "path": "^0.12.7",
    "typeorm": "^0.2.24",
    "apollo-server": "^2.11.0",
    "@nestjs/platform-express": "^6.11.7",
    "apollo-server-express": "^2.11.0",
    "type-graphql": "^0.17.6",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "extract-files": "7.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^6.11.7",
    "@nestjs/core": "^6.11.7",
    "@nestjs/graphql": "^7.1.5",
    "@nestjs/platform-express": "^6.11.7",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.3.1",
    "@nestjsx/crud": "^4.4.2",
    "@nestjsx/crud-typeorm": "^4.4.5",
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "class-transformer": "^0.2.3",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "rxjs": "^6.5.5",
    "supertest": "4.0.2",
    "ts-jest": "25.0.0",
    "ts-node": "8.9.1",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.16.0",
    "typescript": "3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
